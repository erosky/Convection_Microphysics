function [] = plot_two_pdfs(region, corestart, coreend, edgestart, edgeend, numberofbins, titlestring, outputfolder);

[Dcenters1, C1, err1] = pdf_test(region, corestart, coreend, numberofbins)
[Dcenters2, C2, err2] = pdf_test(region, edgestart, edgeend, numberofbins)

"string"
[Dcenters1;Dcenters2]
max([Dcenters1;Dcenters2])

%Plot droplet size distribution in #/cc/um
fig = figure('visible','off');
plot(Dcenters1, C1, 'LineWidth', 2, 'DisplayName','Core')
hold on
er = errorbar(Dcenters1, C1, err1, "o", 'LineWidth', 1, 'Color', 'b',  'HandleVisibility','off');
hold on
plot(Dcenters2, C2, 'LineWidth', 2, 'DisplayName','Edge')
hold on
er = errorbar(Dcenters2, C2, err2, "o", 'LineWidth', 1, 'Color', '#EDB120',  'HandleVisibility','off');
hold on
set(gca,'YScale','log','XScale','log')
xlabel('Diameter (microns)'), ylabel('Concentration (#/cc)')
max(Dcenters1,Dcenters2)
%xlim([12 max(Dcenters1,Dcenters2)])
legend
title(titlestring,  'interpreter', 'none')

saveas(fig, sprintf('%s/%s_dsd.png', outputfolder, titlestring));